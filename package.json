{
  "private": true,
  "name": "btcpayserver-website",
  "version": "1.0.0",
  "license": "MIT",
  "description": "BTCPay Server Official Website",
  "homepage": "https://btcpayserver.org/",
  "repository": "btcpayserver/btcpayserver.github.io",
  "bugs": "https://github.com/btcpayserver/btcpayserver.github.io/issues",
  "scripts": {
    "clean": "rimraf dist transifex",
    "init": "node -r dotenv/config tasks/download_translations.js",
    "build": "npm-run-all -p build:*",
    "build:static": "mkdir -p dist && cp -r source/src/fonts source/src/img source/src/video dist",
    "build:template": "node tasks/render_templates.js"
  },
  "keywords": [
    "BTCPayServer"
  ],
  "devDependencies": {
    "dotenv": "8.2.0",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.0",
    "sync-request": "6.1.0"
  }
}
